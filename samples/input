BEGIN INPUT 
 
    INTERVAL CX1(FLOAT);
    INTERVAL CY1(FLOAT);
    INTERVAL R1(FLOAT);

    INTERVAL CX2(FLOAT);
    INTERVAL CY2(FLOAT);
    INTERVAL R2(FLOAT);

END INPUT ;


BEGIN OUTPUT  
    INTERVAL X(FLOAT);
    INTERVAL Y(FLOAT);
END OUTPUT ;

BEGIN CONSTRAINT

		E1 : R1**2 = (X - CX1)**2 + (Y - CY1)**2;
		E2 : R2**2 = (X - CX2)**2 + (Y - CY2)**2;
END CONSTRAINT;
